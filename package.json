{
  "name": "@haog1/micro-nats",
  "version": "1.1.0",
  "description": "A custom nats-streaming wrapper microservice",
  "engines": {
    "node": ">= 14.0.0"
  },
  "keywords": [
    "express",
    "nats",
    "nats-streaming",
    "microservice",
    "router",
    "middleware"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haog1/micro-nats.git"
  },
  "author": "Tony Gao <gaohaoqian@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haog1/micro-nats/issues"
  },
  "homepage": "https://github.com/haog1/micro-nats#readme",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "prettier --write src/**/*.ts",
    "clean": "del ./build/*",
    "build": "yarn clean && tsc"
  },
  "dependencies": {
    "express": "^4.17.1",
    "node-nats-streaming": "^0.3.2",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.7.10",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "off"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ],
    "*.test.ts": [
      "prettier --write"
    ]
  }
}
